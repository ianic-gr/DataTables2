{
  "name": "@ianicdev/datatables2",
  "version": "0.4.15",
  "type": "module",
  "types": "./lib/index.d.ts",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./dist/datatables.js",
      "require": "./dist/datatables.cjs"
    },
    "./styles.css": "./dist/datatables.css",
    "./locale": {
      "types": "./lib/locale/index.d.ts",
      "import": "./lib/locale/index.js",
      "require": "./lib/locale/index.js"
    }
  },
  "module": "./dist/datatables.js",
  "main": "./dist/datatables.cjs",
  "browser": {
    "./styles.css": "./dist/style.css"
  },
  "license": "MIT",
  "files": [
    "dist/",
    "lib/",
    "lib/index.d.ts",
    "lib/locale.d.ts",
    "lib/locale/index.d.ts"
  ],
  "typesVersions": {
    "*": {
      "*": [
        "lib/index.d.ts"
      ],
      "locale": [
        "lib/locale.d.ts"
      ]
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "rimraf dist && vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --fix"
  },
  "dependencies": {
    "@mdi/font": "7.4.47",
    "@vueuse/core": "^13.1.0",
    "core-js": "^3.42.0",
    "export-to-csv": "^1.4.0",
    "file-saver": "^2.0.5",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "moment": "^2.30.1",
    "print-js": "^1.6.0",
    "roboto-fontface": "*",
    "sortablejs": "^1.15.6",
    "vue": "^3.5.13",
    "vue-i18n": "^11.1.3",
    "vuetify": "^3.8.3",
    "xlsx": "npm:@e965/xlsx@0.20.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@tsconfig/node22": "^22.0.1",
    "@types/node": "^22.15.14",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/tsconfig": "^0.7.0",
    "defu": "^6.1.4",
    "eslint": "^9.26.0",
    "eslint-plugin-vue": "^10.1.0",
    "pinia": "^3.0.2",
    "sass": "^1.87.0",
    "typescript": "^5.8.3",
    "unplugin-auto-import": "^19.2.0",
    "unplugin-fonts": "^1.3.1",
    "unplugin-vue-components": "^28.5.0",
    "unplugin-vue-router": "^0.12.0",
    "vite": "^6.3.5",
    "vite-plugin-vue-layouts-next": "^0.1.1",
    "vite-plugin-vuetify": "^2.1.1",
    "vue-router": "^4.5.1",
    "vue-tsc": "^2.2.10"
  }
}
