{
  "name": "@ianicdev/datatables2",
  "version": "0.10.6",
  "type": "module",
  "types": "./lib/index.d.ts",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./dist/datatables.js",
      "require": "./dist/datatables.cjs"
    },
    "./styles.css": "./dist/datatables2.css",
    "./locale": {
      "types": "./lib/locale/index.d.ts",
      "import": "./lib/locale/index.js",
      "require": "./lib/locale/index.js"
    }
  },
  "module": "./dist/datatables.js",
  "main": "./dist/datatables.cjs",
  "browser": {
    "./styles.css": "./dist/style.css"
  },
  "license": "MIT",
  "files": [
    "dist/",
    "lib/",
    "lib/index.d.ts",
    "lib/locale.d.ts",
    "lib/locale/index.d.ts"
  ],
  "typesVersions": {
    "*": {
      "*": [
        "lib/index.d.ts"
      ],
      "locale": [
        "lib/locale.d.ts"
      ]
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "rm -rf dist && vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --fix",
    "update": "npx npm-check-updates --interactive --format group"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "@vueuse/core": "^14.1.0",
    "@vueuse/integrations": "^14.1.0",
    "core-js": "^3.47.0",
    "export-to-csv": "^1.4.0",
    "file-saver": "^2.0.5",
    "jspdf": "^3.0.4",
    "jspdf-autotable": "^5.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "print-js": "^1.6.0",
    "qs": "^6.14.0",
    "roboto-fontface": "*",
    "sortablejs": "^1.15.6",
    "vue": "^3.5.25",
    "vue-i18n": "^11.2.2",
    "vuetify": "^3.11.1",
    "xlsx": "npm:@e965/xlsx@0.20.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tsconfig/node22": "^22.0.5",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "defu": "^6.1.4",
    "eslint": "^9.39.1",
    "eslint-plugin-vue": "^10.6.2",
    "pinia": "^3.0.4",
    "sass": "^1.94.2",
    "typescript": "^5.9.3",
    "unplugin-auto-import": "^20.3.0",
    "unplugin-fonts": "^1.4.0",
    "unplugin-vue-components": "^30.0.0",
    "unplugin-vue-router": "^0.18.0",
    "vite": "^7.2.6",
    "vite-plugin-vue-layouts-next": "^1.3.0",
    "vite-plugin-vuetify": "^2.1.2",
    "vue-router": "^4.6.3",
    "vue-tsc": "^3.1.5"
  }
}
